<div class="container p-3">

    <table class="table table-striped table-hover table-dark">
        <thead>
          <tr>
            
            <th scope="col">Item</th>
            <th scope="col">Quantity</th>
          </tr>
        </thead>
        <tbody>
          <% for(product of products){%>
          <tr>
            <td><%=product.name%></td>
            <td><%=product.quantity%></td>
            </tr>
            <%}%>
        </tbody>
      </table>
</div>